{
  "mcpServers": {
    "exa": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-remote",
        "https://mcp.exa.ai/mcp?tools=web_search_exa,get_code_context_exa,crawling_exa"
      ]
    },
    "claude-context": {
      "command": "npx",
      "args": [
        "-y",
        "@zilliz/claude-context-mcp"
      ]
    },
    "cipher": {
      "command": "cipher",
      "args": [
        "--mode",
        "mcp",
        "--agent",
        ".cipher/config.yml"
      ]
    },
    "mcp-funnel": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-funnel@0.0.6"
      ]
    }
  }
}
